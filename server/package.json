{
  "name": "environmentmanager",
  "version": "3.31.2",
  "description": "EnvironmentManager Server",
  "homepage": "https://trainline.github.io/environment-manager/",
  "bugs": {
    "url": "https://github.com/trainline/environment-manager/issues",
    "email": "platform.development@thetrainline.com"
  },
  "config": {
    "git-tag-version": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trainline/environment-manager.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "debug": "nodemon --debug-brk index.js",
    "dev": "nodemon index.js",
    "lint": "eslint . --ext .js --quiet",
    "lint-rule": "eslint . --ext .js --quiet --parser-options {ecmaVersion:6} --rule ",
    "lint-and-test": "npm run lint -s && npm test -s",
    "test": "mocha --recursive ./test --EM_PROFILE test/test-profile.json",
    "test-watch": "mocha --watch --recursive ./test --EM_PROFILE test/test-profile.json",
    "test-ci": "npm test -- --reporter mocha-teamcity-reporter --timeout 4000",
    "mocha": "mocha",
    "acceptance-tests": "mocha --recursive ./acceptance-tests --EM_PROFILE acceptance-tests/test-profile.json"
  },
  "author": {
    "name": "Platform Development",
    "email": "platform.development@thetrainline.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "activedirectory": "^0.7.2",
    "ajv": "^4.9.1",
    "app-module-path": "^2.1.0",
    "archiver": "^1.2.0",
    "async": "^2.1.4",
    "aws-sdk": "^2.7.10",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "cache-manager": "^2.3.0",
    "co": "^4.6.0",
    "compression": "^1.6.2",
    "consul": "^0.27.0",
    "cookie-parser": "^1.4.3",
    "deep-freeze": "0.0.1",
    "es6-template-strings": "^2.0.1",
    "express": "^4.14.0",
    "express-request-id": "^1.3.0",
    "express-winston": "^2.1.0",
    "glob-intersection": "^0.1.3",
    "ioredis": "^2.5.0",
    "ip": "^1.1.4",
    "js-joda": "^1.1.17",
    "js-yaml": "^3.7.0",
    "jsondiffpatch": "^0.2.4",
    "jsonwebtoken": "^7.1.9",
    "later": "^1.2.0",
    "lodash": "^4.17.2",
    "md5": "^2.2.1",
    "moment": "^2.17.1",
    "ms": "^0.7.2",
    "mz": "^2.6.0",
    "nconf": "^0.8.4",
    "newrelic": "^1.36.2",
    "node-uuid": "^1.4.7",
    "pluralize": "^3.0.0",
    "request": "^2.79.0",
    "require-directory": "^2.1.1",
    "requirejs": "^2.3.2",
    "retry": "^0.10.0",
    "s3-streams": "^0.3.0",
    "semver": "^5.3.0",
    "swagger-tools": "0.10.1",
    "through2": "^2.0.3",
    "through2-map": "^3.0.0",
    "unzip2": "^0.2.5",
    "valid-url": "^1.0.9",
    "winston": "^2.3.0",
    "xmldom": "^0.1.27",
    "xpath": "0.0.23"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "mocha": "^3.2.0",
    "mocha-teamcity-reporter": "^1.1.1",
    "proxyquire": "^1.7.10",
    "rewire": "^2.5.2",
    "should": "^11.1.1",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1",
    "yargs": "^6.5.0"
  },
  "engines": {
    "node": ">=6.7.0",
    "npm": ">=3.10.3"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "jshintConfig": {
    "esversion": 6,
    "globals": {
      "setInterval": true,
      "Buffer": true,
      "console": true,
      "process": true,
      "global": true,
      "__dirname": true,
      "require": true,
      "module": true,
      "describe": true,
      "before": true,
      "after": true,
      "it": true
    },
    "undef": true,
    "strict": "global"
  }
}
